{
    "configurations": [
        {
            "name": "RISCV Kernel",
            "includePath": [
                "${workspaceFolder}/include/",
                "${workspaceFolder}/include/std/",
                "${workspaceFolder}/third_party/include/",
                "${workspaceFolder}/kernel/"
            ],
            "defines": [
                "_DEBUG",
                "BITS=64"
            ],
            "compilerPath": "riscv64-unknown-elf-gcc",
            "cStandard": "c23",
            "cppStandard": "gnu++20",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        },
        {
            "name": "RISCV Libs",
            "includePath": [
                "${workspaceFolder}/include/",
                "${workspaceFolder}/include/std/",
                "${workspaceFolder}/third_party/include/"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "riscv64-unknown-elf-gcc",
            "cStandard": "c23",
            "cppStandard": "gnu++20",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}